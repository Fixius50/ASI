{
  "trailingSlash": true,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)\\.(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/jetson/", "destination": "/jetson_nano/" 
    },
    {
      "source": "/jetson/:match*", "destination": "/jetson_nano/:match*"
    },
    {
      "source": "/mac/", "destination": "/mac_raspi/" 
    },
    {
      "source": "/mac/:match*", "destination": "/mac_raspi/:match*"
    },
    {
      "source": "/comparacion/", "destination": "/comparacion_modelos/" 
    },
    {
      "source": "/comparacion/:match*", "destination": "/comparacion_modelos/:match*"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/comparacion/",
      "permanent": false
    },
    {
      "source": "/favicon.ico",
      "destination": "https://via.placeholder.com/1",
      "permanent": true
    }
  ]
}