{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/projects/(.*)",
      "destination": "/projects/$1"
    },
    {
      "source": "/([^/\\.]+)",
      "destination": "/$1/index.html"
    },
    {
      "source": "/([^/\\.]+)/(.*)",
      "destination": "/$1/$2"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}